using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter number: ");
        int n = Convert.ToInt32(Console.ReadLine());
        int temp = n, rev = 0;

        while (n != 0)
        {
            rev = rev * 10 + n % 10;
            n /= 10;
        }

        if (temp == rev)
            Console.WriteLine("Palindrome");
        else
            Console.WriteLine("Not Palindrome");
    }
}
