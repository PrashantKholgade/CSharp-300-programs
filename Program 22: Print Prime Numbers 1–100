using System;

class Program
{
    static void Main()
    {
        for (int n = 2; n <= 100; n++)
        {
            bool isPrime = true;

            for (int i = 2; i <= n / 2; i++)
            {
                if (n % i == 0)
                {
                    isPrime = false;
                    break;
                }
            }

            if (isPrime)
                Console.WriteLine(n);
        }
    }
}
